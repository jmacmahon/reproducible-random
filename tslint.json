{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-standard"
    ],
    "linterOptions": {
      "excludes": [
        "node_modules/**/*.ts"
      ]
    },
    "jsRules": {},
    "rules": {
      "no-unused-variable": false,
      "no-unsafe-any": true,
      "await-promise": [true, "Bluebird"],
      "trailing-comma": [true, {"multiline": "always", "singleline": "never"}]
    },
    "rulesDirectory": []
}
